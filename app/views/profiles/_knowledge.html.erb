<div class="row col-md-12 row-knowledge">
  <% @data["knowledge"].each do |info, key| %>
    <div class="col-md-5 wrap-knowledge" v-for="(value, key) in knowledgeInfo">
      <h5 class="text-left"><%= info %></h5>
      <div class="progress">
        <div class="progress-bar bg-success" role="progressbar"
          aria-valuenow="25" aria-valuemin="0"
          aria-valuemax="100" style="width: <%= key.to_s + '%' %>"></div>
      </div>
    </div>
  <% end %>
</div>
